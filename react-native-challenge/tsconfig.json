{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "esModuleInterop": true,
    "jsx": "react",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@components/*": ["mobile/components/*"],
      "@redux/*": ["mobile/redux/*"],
      "@screens/*": ["mobile/screens/*"],
      "@styles/*": ["mobile/styles/*"],
      "@utils/*": ["mobile/utils/*"]
    }
  }
}
